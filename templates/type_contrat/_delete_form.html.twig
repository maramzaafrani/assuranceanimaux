{% block body %}
<form method="post" action="{{ path('app_typecontrat_delete', {'id': typecontrat.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ typecontrat.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
{% endblock %}
